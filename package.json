    "fastify": "^4.24.0",
    "wisp-server-node": "^1.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "nodemon": "^3.0.0",
    "terser": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
