{
  "name": "hyperglass-proxy",
  "version": "2.0.0",
  "description": "A fast and progressive web proxy based on Ultraviolet",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "NODE_ENV=development node --watch src/index.js",
    "build": "node scripts/build.js",
    "test": "node --test",
    "lint": "eslint src/",
    "clean": "rm -rf dist/ node_modules/.cache/",
    "optimize": "node scripts/optimize-assets.js"
  },
  "dependencies": {
    "@fastify/compress": "^7.0.3",
    "@fastify/cors": "^9.0.0",
    "@fastify/helmet": "^11.1.1",
